{
    "ebay_api_app_id": "$EBAY_API_APP_ID",
    "last_run": "ISO 8601 datetime in UTC",
    "search_profile": {
        "category": "139973",
        "filters": [
            {
                "name": "MinPrice",
                "value": "100"
            }
        ],
        "output_selectors": [
            "PictureURLLarge"
        ],
        "search_terms": [
            {
                "clause": "all",
                "keywords": "all the keywords must match"
            },
            {
                "clause": "any",
                "keywords": "any of the keywords can match"
            },
            {
                "clause": "not",
                "keywords": "none of the keywords can match"
            }
        ]
    },
    "twitter_api_config": {
        "access_token_key": "$TWITTER_ACCESS_TOKEN_KEY",
        "access_token_secret": "$TWITTER_ACCESS_TOKEN_SECRET",
        "consumer_key": "$TWITTER_CONSUMER_KEY",
        "consumer_secret": "$TWITTER_CONSUMER_SECRET"
    },
    "bitly_token": "$BITLY_ACCESS_TOKEN"
}
